<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
  <context id="context1" targetRuntime="MyBatis3" defaultModelType="flat">
  	<!-- 数据库连接 -->
    <jdbcConnection connectionURL="jdbc:mysql://localhost:3306/sms?useUnicode=true&amp;characterEncoding=utf-8&amp;
    	useSSL=false&amp;allowPublicKeyRetrieval=true&amp;serverTimezone=Asia/Shanghai" 
    	driverClass="com.mysql.cj.jdbc.Driver" password="root" userId="root">
   	</jdbcConnection>
    <!-- Java实体类所在包名称和项目名称 -->
    <javaModelGenerator targetPackage="com.lqs.sms.entity" targetProject="SMSSend/src/main/java" />
    <!-- mapper.xml所在的包(targetPackage)和所在的项目(targetProject) -->
    <sqlMapGenerator targetPackage="mapping" targetProject="SMSSend/src/main/resources"></sqlMapGenerator>
    <!-- mapper所在的包(targetPackage)和所在的项目(targetProject) -->
    <javaClientGenerator targetPackage="com.lqs.sms.mapper" targetProject="SMSSend/src/main/java" type="XMLMAPPER" />
    <!-- tableName标签表示数据库中的表，可以有多个，
         
     -->
    <table schema="Phone" tableName="phone">
      
    </table>
  </context>
</generatorConfiguration>